{
  "name": "gaming-duo-bot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:tsnode": "nodemon --watch src --exec ts-node --esm src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "generate:ankets": "tsx src/scripts/generateTestAnkets.ts",
    "test": "tsx --test tests/billing.test.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "pino": "^9.3.2",
    "prisma": "^5.16.1",
    "telegraf": "^4.16.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.7.2",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.7",
    "typescript": "^5.6.2"
  }
}
